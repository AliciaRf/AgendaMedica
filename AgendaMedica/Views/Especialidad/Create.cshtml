@model AgendaMedica.Models.Especialidad
@{
    ViewData["Title"] = "Crear Especialidad";
}
<h1>Crear Especialidad</h1>
<div>
    <form asp-action="Create">
        <div> 
        <label class="form-label"></label>
            <input type="text" class="form-control" asp-for="Especialidad1" />
        </div>
        <div class="mt-3 text-end">
            <a asp-action="Index" class="btn btn-outline-info">Volver</a>
    
        <input type="submit" value="Crear" class="btn btn-outline-success" />
        </div>

    </form>
</div>
