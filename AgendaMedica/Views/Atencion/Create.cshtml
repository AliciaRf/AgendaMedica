@model AgendaMedica.Models.Atencion
@{
    ViewData["Title"] = "Ingresar tipo de Atencion";
}
<h1>Ingresar tipo de Atencion</h1>
<div>
    <form asp-action="Create">
        <div>
            <label class="form-label"></label>
            <input type="text" class="form-control" asp-for="NombreAte" />
        </div>
        <div class="mt-3 text-end">
            <a asp-action="Index" class="btn btn-outline-info">Volver</a>

            <input type="submit" value="Crear" class="btn btn-outline-success" />
        </div>

    </form>
</div>